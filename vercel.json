{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.9",
          "framework": "flask",
          "entrypoint": "app.py",
          "installCommand": "pip install -r requirements.txt",
          "buildCommand": "python -m pip install --upgrade pip && pip install -r requirements.txt",
          "maxDuration": 30
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }